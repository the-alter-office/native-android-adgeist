<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- AdView custom attributes -->
    <declare-styleable name="AdView">
        <attr name="adUnitId" format="string" />
        
        <attr name="adSize" format="enum">
            <enum name="banner" value="0" />
            <enum name="largeBanner" value="1" />
            <enum name="mediumRectangle" value="2" />
            <enum name="fullBanner" value="3" />
            <enum name="leaderboard" value="4" />
            <enum name="wideSkyscraper" value="5" />
        </attr>
    </declare-styleable>
</resources>
